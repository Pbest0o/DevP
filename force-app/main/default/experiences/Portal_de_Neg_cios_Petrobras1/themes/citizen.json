tyle: italic;\n  }\n  \n\n/* Font Definition */\n\n\n.guest-information-tile{\n    color: #373737 !important;\n}\n.slds-p-bottom_xx-small, .slds-p-bottom--xx-small {\n    display: none;\n}\nspan .inverseTextColor {\n    color: #000000 !important;\n}\n\n.forceCommunityThemeHeaderCompact .themeUtils .themeUtil svg {\n    height: 25px !important;\n}\n.themeProfileMenu .ui-widget {\n    display:inline-block !important;\n}\n.slds-table.slds-table_cell-buffer.slds-table_bordered.slds-table_col-bordered.slds-border--left.slds-border--right {\n    display:none;\n}\n.slds-icon_container.slds-icon-utility-chevronright {\ndisplay:none;\n}\n\n\n/* Accordion Stylesheet */\n.faq {\n    position: absolute;\n    opacity: 0;\n    z-index: -1;\n}\n.tab {\n    width: 100%;\n    border-top: 1px solid var(--neutral-400, #EEE);\n    overflow: hidden;\n}\n.tab-label {\n    display: flex;\n    padding: 24px 32px;\n    color: var(--neutral-800, #373737);\n    font-weight: 700;\n    font-size: 24px;\n    cursor: pointer;\n    justify-content: space-between;\n}\n/* Icon */\n.tab-label::after {\n    content: \"\\276f\";\n    width: 1em;\n    height: 100%;\n    text-align: center;\n    transition: all 0.35s;\n    transform: rotate(90deg);\n}\n.tab-content {\n    max-height: 0;\n    padding: 0 32px;\n    transition: all 0.35s;\n    font-size: 16px;\n    color: var(--neutral-800, #373737);\n}\n.faq:checked + .tab-label:after {\n    transform: rotate(-90deg);\n}\n.faq:checked ~ .tab-content {\n    max-height: 100vh;\n    padding: 0 32px 32px;\n}\n\n/* Esconder banner da homepage em mobile*/\n@media screen and (max-width: 700px) {\n  .homepage-banner {\n    display: none;\n  }\n}\n\n/* Esconder Icone Padrão na pagina de manifestações */\n.comm-page-list-500 .highlights-icon-container {\n    display: none;\n}\n.comm-page-list-500 .slds-breadcrumb {\n    display: none;\n}\n\n/* Decoração do botao de criar nova manifestação*/\n.manifestacao-button:hover {\n\ttext-decoration: none !important;\n}\n\n/* Ajuste dos acessos rápidos */\n\n.container {\n    display: flex;\n    flex-wrap: wrap;\n}\n.card {\n    width: calc(16.66% - 20px); /* 16.66% para cada card */\n    margin: 8px;\n    min-width: 200px; /* Largura mínima do card */\n}\n\n@media screen and (max-width: 1200px) {\n    .card {\n        width: calc(33.33% - 20px); /* 33.33% para cada card em telas menores que 1200px */\n    }\n}\n\n@media screen and (max-width: 768px) {\n    .card {\n        width: calc(50% - 20px); /* 50% para cada card em telas menores que 768px */\n    }\n}\n\n@media screen and (max-width: 576px) {\n    .card {\n        width: calc(100% - 20px); /* 100% para cada card em telas menores que 576px */\n    }\n}\n\n/* Ajuste Tela de login */\n.salesforceIdentityLoginBody2 {\n    box-shadow: inset 0 0 0 1000px rgba(0, 133, 66, 0.30);\n}\n\n.salesforceIdentityLoginBody2 .cCenterPanel .siteforceContentArea {\n    background-color: #FFF !important;\n    border: 1px solid #D8DDE6 !important;\n    border-radius: 8px;\n    padding: 34px 48px !important;\n}\n\n.salesforceIdentityLoginBody2 .ui-widget {\n    padding: 0px !important; /* Remover padding padrão do ui-widget */\n}\n.salesforceIdentityLoginBody2 .slds-col_padded, .slds-col--padded {\n    /*padding: 0px !important;*/ /* remover padding padrão adicionado pela plataforma */\n}\n\n@media screen and (min-width: 400px) {\n    .salesforceIdentityLoginBody2 .cCenterPanel {\n        max-width: 400px !important; /* ajustar max-width padrão da plataforma */    \n    }\n\n    .salesforceIdentityLoginBody2 .cCenterPanel .siteforceContentArea {\n        width:400px !important;\n    }\n    \n    /* Ajuste de botão */\n    .salesforceIdentityLoginForm2 .uiButton {\n        margin-left: 50% !important;\n    \ttransform: translate(-50%, 0%) !important;\n        width: 213px !important;\n\t}\n}\n@media screen and (max-width: 399px) {\n    .salesforceIdentityLoginBody2 .cCenterPanel {\n\t\tmax-width: 290px !important; /* ajustar max-width padrão da plataforma */    \n\t}\n\t.salesforceIdentityLoginBody2 .cCenterPanel .siteforceContentArea {\n\t\twidth:290px !important;\n\t}\n}\n\n.salesforceIdentityLoginForm2 .inputIcon {\n    display: none; /* Remover Icones Padrão da plataforma*/\n}\n\n.salesforceIdentityLoginForm2 .inputBox{\n    margin: 0px !important;\n    width: 100% !important;\n    padding: 16px !important;\n    border-radius: 8px;\n    border: 1px solid #D8DDE6 !important;\n}\n\n.salesforceIdentityLoginForm2 .inputContainer {\n    border-radius: 8px !important;\n    padding: 0px !important;\n    border:0px !important;\n    background-color: transparent !important;\n}\n\n.salesforceIdentityLoginForm2 input::placeholder {\n\tcolor: transparent; /* remover placeholder padrão */\n}\n\n.salesforceIdentityLoginForm2 .inputBox {\n    height: 43px !important;\n}\n\n.salesforceIdentityLoginForm2 .uiButton {\n    border-radius: 100px !important;\n    margin-top: 40px !important;\n    margin-bottom: 13px !important;\n}\n\n.salesforceIdentityLoginForm2 #sfdc_username_container {\n    margin-top: 55px;\n    margin-bottom: 18px;\n}\n\n.salesforceIdentityLoginForm2 label.uiLabel-hidden {\n    display: block !important;\n    position: relative !important;\n    top: -66px !important;\n}\n\n.salesforceIdentityLoginForm2 .assistiveText {\n    position: relative !important;\n    color: #75787B;\n}\n\n.salesforceIdentityLoginForm2 .inverseTextColor {\n    color: #008542 !important;\n    position: relative;\n    top: -125px;\n}\n\n.salesforceIdentityLoginForm2 div:has(a){\n    text-align:right;\n}\n\n/* Estilos para o container dos customCards */\n.customCard-container {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center; /* Alinha os customCards no centro horizontal */\n    width: 80%; /* Ocupa toda a largura da página */\n    margin: 0 auto; /* Centraliza horizontalmente */\n}\n\n/* Estilos para o cartão */\n.customCard {\n    border-radius: 16px;\n    border: 1px solid var(--neutral-400, #EEE);\n    background: var(--neutral-100, #FFF);\n    width: calc(34% - 20px); /* Ajuste a largura dos customCards, considerando a margem */\n    display: flex;\n    flex-direction: column;\n    margin: 20px; /* Adicione espaçamento entre os customCards */\n    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1); /* Sombra padrão do card */\n    transition: box-shadow 0.3s ease; /* Transição suave para a sombra */\n}\n\n@media screen and (max-width: 1200px) {\n    .customCard {\n        width: calc(33.33% - 20px); /* 33.33% para cada card em telas menores que 1200px */\n    }\n}\n\n@media screen and (max-width: 768px) {\n    .customCard {\n        width: calc(50% - 20px); /* 50% para cada card em telas menores que 768px */\n    }\n}\n\n@media screen and (max-width: 576px) {\n    .customCard {\n        width: calc(100% - 20px); /* 100% para cada card em telas menores que 576px */\n    }\n}\n\n.customCard:hover {\n  box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.2); /* Nova sombra ao passar o mouse */\n}\n\n/* Estilos para a imagem no topo do cartão */\n.img1 {\n    max-width: 100%;\n    height: 70%; /* Ajuste a altura da imagem conforme necessário */\n    width: auto; /* Define a largura da imagem proporcionalmente */\n    border-top-left-radius: 16px;\n    border-top-right-radius: 16px;\n    object-fit: cover; /* Preenche toda a área do elemento com a imagem */\n}\n\n.img2 {\n    max-width: 100%;\n    height: 52.5%; /* Ajuste a altura da imagem conforme necessário */\n    width: auto; /* Define a largura da imagem proporcionalmente */\n    border-top-left-radius: 16px;\n    border-top-right-radius: 16px;\n    object-fit: cover; /* Preenche toda a área do elemento com a imagem */\n}\n\n.img3 {\n    border-top-left-radius: 16px;\n    border-top-right-radius: 16px;\n    object-fit: cover; /* Preenche toda a área do elemento com a imagem */\n}\n\n/* Estilos para o conteúdo do cartão */\n.customCard-content {\n    padding: 16px;\n}\n\n.customCard-content p {\n    color: black; /* Define a cor do texto como preto */\n    font-size: 16px; /* Aumenta o tamanho da fonte do texto */\n}\n\n.linha-amarela {\n    border-bottom: 3px solid #FFD700;\n    width: 24px; /* Ajuste a largura da linha conforme desejado */\n    display: inline-block;\n}\n\n.btn-container {\n  display: inline-block;\n  position: relative;\n}\n\n.btn {\n  display: inline-block;\n  padding: 8px 16px;\n  font-size: 16.5px;\n  font-weight: bold; /* Texto em negrito */\n  text-decoration: underline;\n  color: darkgreen;\n  background-color: transparent;\n  border-radius: 19px; /* Bordas mais arredondadas */\n  transition: background-color 0.3s ease, box-shadow 0.3s ease; /* Transições suaves */\n  padding-left: 8px;\n}\n\n\n@media screen and (max-width: 1400px) {\n  .logo-petro{\n    margin: auto;\n      /*width: 50%;*/\n      padding: 10px;\n  }\n    \n    .sac{\n        margin: auto;\n      \twidth: 50%;\n      \tpadding: 10px;\n    }\n    \n    .social-petro{\n        margin: auto;\n      \t/*width: 50%;*/\n      \tpadding-top: 10px;\n    }\n    \n    .copy-petro{\n        margin: auto;\n      \tpadding: 10px;\n    }\n    \n    .horinbar-petro{\n\t\tmargin:0 20px !important;\n    }\n    \n    \n}\n\n@media screen and (max-width: 767px) {\n\t.forceCommunityThemeLogo .logoImage{\t\n       width: 200px !important;\n        height: 50px !important;       \n    \n    }\n    .forceCommunityThemeHeaderCompact .themeNav{\n        height:500px;\n    }\n    \n}\n.slds-carousel__indicator-action.slds-is-active, .slds-carousel__indicator-action.slds-is-active:hover{\n  color: darkgreen;  \n  background-color: darkgreen;\n  border: 2px solid #c0c0c0;\n  box-sizing: border-box;\n}\n\n\n.slds-carousel__panel-action {\n    display: block;\n    border: none;\n}\n.slds-box {\n     border: none;\n\t \n}\n\n.slds-carousel__image>img {\n    width: 200%;\n}\n\n/* Ajuste Banner Guest User */\n\n.siteforceContentArea .comm-layout-column .ui-widget:not(:last-child), .siteforceContentArea .cb-section_column .ui-widget:not(:last-child){\n    margin-bottom: 0 !important;\n}\n\n\n/*\n.siteforceThemeLayoutStarter * {\n    border: none;\n}*/\n\n/*.btn:hover {\n\tbackground-color: #E5FFF6; \n    box-shadow: 0 4px 8px rgba(0, 128, 0, 0.3);\n    transform: translateX(17px);\n}*/",
